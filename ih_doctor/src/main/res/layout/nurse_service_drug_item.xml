<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dp_10"
    android:background="@drawable/nurse_service_item_bg">

    <TextView
        android:id="@+id/base_info_tip_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/common_bottom_line_bg"
        android:drawableLeft="@mipmap/point_green"
        android:drawablePadding="@dimen/dp_10"
        android:paddingLeft="@dimen/dp_17"
        android:paddingTop="@dimen/dp_15"
        android:paddingRight="@dimen/dp_17"
        android:paddingBottom="@dimen/dp_15"
        android:text="第一次器材/耗材耗用"
        android:textColor="@color/fontColorPrimary"
        android:textSize="@dimen/font_size_primary" />

    <LinearLayout
        android:id="@+id/service_order_detail_ll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/base_info_tip_tv"
        android:layout_marginTop="@dimen/dp_8"
        android:orientation="vertical"
        android:paddingLeft="@dimen/dp_16"
        android:paddingRight="@dimen/dp_16"></LinearLayout>


</RelativeLayout>