<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_72">
    <TextView
        android:id="@+id/templet_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/dp_28"
        android:drawableLeft="@mipmap/point_green"
        android:drawablePadding="@dimen/dp_13"
        android:textSize="@dimen/font_size_primary"
        android:layout_centerVertical="true"
        android:textColor="#333333"
        android:text="流感（普通）"/>
    <ImageView
        android:layout_width="@dimen/dp_10"
        android:layout_height="@dimen/dp_20"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@mipmap/consulting_arraw_right"
        android:layout_marginEnd="@dimen/dp_31"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="#EEEEEE"
        android:layout_alignParentBottom="true"/>

</RelativeLayout>