    <declare-styleable name="generaltitle">
        <attr name="leftImgRec" format="integer" />
        <attr name="titleName" format="string" />
        <attr name="rightString" format="string" />
        <attr name="rightDrawableTop" format="integer" />
        <attr name="isRightTvVisiable" format="boolean" />
        <attr name="isLeftImgVisiable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="width_space" format="dimension" />
        <attr name="height_space" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RatingBarView">

        <attr name="starImageSize" format="dimension" />
        <attr name="starCount" format="integer" />
        <attr name="starEmpty" format="reference" />
        <attr name="starFill" format="reference" />
        <attr name="canTouch" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ChildCareItemLayout">
        <attr name="item_image" format="reference" />
        <attr name="item_title" format="reference" />
    </declare-styleable>

    <declare-styleable name="MedicalAuthLayout">
        <attr name="tvHint" format="reference" />
    </declare-styleable>

    <declare-styleable name="RedStarTextView">
        <attr name="android:text" />
        <attr name="starType" format="string|reference" />
        <attr name="starColor" format="color|reference" />
    </declare-styleable>

</resources>