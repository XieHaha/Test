<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
    android:id="@+id/unorder_exa_name_tv"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_toRightOf="@+id/head_layout"
    android:layout_marginTop="@dimen/dp_24"
    android:layout_marginStart="@dimen/dp_19"
    android:text="放射"
    android:textSize="@dimen/sp_14"
    android:textColor="#666666"/>
    <TextView
        android:id="@+id/unorder_exa_dsc_tv"
        android:layout_below="@+id/unorder_exa_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/head_layout"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginStart="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_24"
        android:text="检查项目：胸部正位、胸腔X光"
        android:textSize="@dimen/dp_12"
        android:textColor="#BBBBBB"/>
    <TextView
        android:id="@+id/unorder_exa_order_tv"
        android:layout_width="@dimen/dp_72"
        android:layout_height="@dimen/dp_31"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:gravity="center"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/primary_color"
        android:text="预约"
        android:background="@mipmap/fill_personal_bt_bg"
        android:layout_marginEnd="@dimen/dp_51" />
</RelativeLayout>